[core]
    excludesfile = ~/.gitignore
[user]
    name = Richard Crosby
[push]
    default = current
[alias]
    s = status -s
    c = commit -m
    a = add
    aa= !git add -u && git add . && git status -s
    start = checkout -b
    up = !git fetch origin && git rebase origin/master
    ir = rebase -i origin/master
    history = !git log --decorate=short --pretty=oneline
[commit]
    gpgsign = true
